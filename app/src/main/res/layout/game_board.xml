<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#01579B">

    <ImageButton
        android:padding="20dp"
        android:id="@+id/b9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"

        app:layout_constraintStart_toEndOf="@+id/b8"
        app:layout_constraintTop_toBottomOf="@+id/b6" />

    <ImageButton
        android:padding="20dp"
        android:id="@+id/b8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b9"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/b7"
        app:layout_constraintTop_toBottomOf="@+id/b5" />

    <ImageButton

        android:padding="20dp"
        android:id="@+id/b7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/b8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b4" />


    <ImageButton
        android:padding="20dp"
        android:id="@+id/b6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/b9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/b5"
        app:layout_constraintTop_toBottomOf="@+id/b3" />

    <ImageButton
        android:padding="20dp"
        android:id="@+id/b5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/b8"
        app:layout_constraintEnd_toStartOf="@+id/b6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/b4"
        app:layout_constraintTop_toBottomOf="@+id/b2" />

    <ImageButton
        android:padding="20dp"
        android:id="@+id/b4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/b7"
        app:layout_constraintEnd_toStartOf="@+id/b5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b1" />

    <ImageButton
        android:padding="20dp"
        android:id="@+id/b1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/b4"
        app:layout_constraintEnd_toStartOf="@+id/b2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:padding="20dp"
        android:id="@+id/b2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/b5"
        app:layout_constraintEnd_toStartOf="@+id/b3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/b1"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:padding="20dp"
        android:id="@+id/b3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:background="#fff"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/b6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/b2"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/set5"
        android:layout_width="10dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/stick_color"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="@+id/b5"
        app:layout_constraintStart_toStartOf="@+id/b5" />
    <View
        android:id="@+id/set2"
        android:background="@color/stick_color"
        android:layout_width="match_parent"
        android:visibility="invisible"
        android:layout_gravity="center"
        android:layout_height="10dp"
        app:layout_constraintTop_toTopOf="@+id/b5"
        app:layout_constraintBottom_toBottomOf="@+id/b5"
        />
    <View
        android:id="@+id/set4"
        android:layout_width="10dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/stick_color"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="@+id/b4"
        app:layout_constraintStart_toStartOf="@+id/b4" />
    <View
        android:id="@+id/set6"
        android:layout_width="10dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@color/stick_color"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="@+id/b6"
        app:layout_constraintStart_toStartOf="@+id/b6" />

    <View
        android:id="@+id/set1"
        android:background="@color/stick_color"
        android:layout_width="match_parent"
        android:visibility="invisible"
        android:layout_gravity="center"
        android:layout_height="10dp"
        app:layout_constraintTop_toTopOf="@+id/b2"
        app:layout_constraintBottom_toBottomOf="@+id/b2"
        />
    <View
        android:id="@+id/set3"
        android:background="@color/stick_color"
        android:layout_width="match_parent"
        android:visibility="invisible"
        android:layout_gravity="center"
        android:layout_height="10dp"
        app:layout_constraintTop_toTopOf="@+id/b8"
        app:layout_constraintBottom_toBottomOf="@+id/b8"
        />
    <View
        android:id="@+id/set7"
        android:background="@color/stick_color"
        android:rotation="138"
        android:layout_width="10dp"
        android:visibility="invisible"
        android:layout_height="380dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <View
        android:id="@+id/se"
        android:background="#01579B"

        android:layout_width="2dp"
        android:visibility="invisible"
        android:layout_height="match_parent"
        app:layout_constraintStart_toEndOf="@+id/b4"
        app:layout_constraintEnd_toStartOf="@+id/b5"
        />
    <View
        android:id="@+id/set8"

        android:background="@color/stick_color"
        android:rotation="43"
        android:layout_width="10dp"
        android:visibility="invisible"
        android:layout_height="380dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
</android.support.constraint.ConstraintLayout>